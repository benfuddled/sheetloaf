{
  "name": "sheetloaf",
  "version": "0.11.3",
  "description": "freshmade stylesheets for the whole family.",
  "main": "./index.js",
  "bin": {
    "sheetloaf": "./index.js"
  },
  "scripts": {
    "test": "mocha",
    "test2": "node . \"test/samples/styles/**/*.scss\" --dir \"test/samples/render/\" --load-path \"test/samples/lib\" --style expanded --base test/samples/styles/ --poll 500 --config test/samples/",
    "test3": "cat test/samples/styles/file.scss | node . --style compressed --use autoprefixer --load-path test/samples/styles > test/samples/render/file.css",
    "test4": "cat test/samples/styles/file-with-error.scss | node . --style compressed --use autoprefixer > test/samples/render/file.css 2> test/samples/logs/error.log"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cheddarbread/sheetloaf.git"
  },
  "files": [
    "index.js",
    "parser.js"
  ],
  "keywords": [
    "scss",
    "css",
    "sass",
    "processor"
  ],
  "author": "Benjamin Richardson",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cheddarbread/sheetloaf/issues"
  },
  "homepage": "https://github.com/cheddarbread/sheetloaf#readme",
  "dependencies": {
    "chokidar": "^3.5.2",
    "colorette": "^1.2.2",
    "commander": "^6.0.0",
    "fast-glob": "^3.2.6",
    "picomatch": "^2.3.0",
    "sass": "^1.35.1"
  },
  "peerDependencies": {
    "postcss": "^8.2.0"
  },
  "devDependencies": {
    "autoprefixer": "^10.2.6",
    "lodash": "^4.17.21",
    "mocha": "^8.4.0",
    "postcss": "^8.3.5",
    "postcss-custom-properties": "^10.0.0"
  }
}
